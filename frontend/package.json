{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "start:ssr": "ng run frontend:serve-ssr",
    "build": "ng build",
    "build:ssr": "ng build",
    "build:ssg": "ng build --prerender",
    "serve:ssr": "node dist/frontend/server/server.mjs",
    "serve:ssg": "npx http-server dist/frontend/browser -p 4000",
    "dev:ssr": "ng run frontend:serve-ssr",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prerender": "ng build --prerender"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.0",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-browser-dynamic": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/service-worker": "^20.2.0",
    "@angular/ssr": "^20.2.0",
    "@fastify/static": "^8.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "fastify": "^5.6.1",
    "apollo-angular": "^11.0.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2002.0",
    "@angular-devkit/core": "^20.2.0",
    "@angular-devkit/schematics": "^20.2.0",
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@schematics/angular": "^20.2.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
